/*  Write a function for arithmetic operators(+,-,*,/)  */

import java.util.Scanner;
public class Main
{
    
	public static void main(String[] args) {
		Main m = new Main();
		Scanner sc = new Scanner(System.in);
		int a= sc.nextInt();
		int b = sc.nextInt();
		m.add(a,b);
		m.sub(a,b);
		m.multiply(a,b);
		m.divison(a,b);
	}
	public void add(int a,int b){
	    int c=a+b;
	    System.out.println("The added value is "+c);
	}
	
	public void sub(int a,int b){
	    int c=a-b;
	    System.out.println("The subtracted value is "+c);
	}
	
	public void multiply(int a,int b){
	    int c=a*b;
	    System.out.println("The multiply value is "+c);
	}
	
	public void divison(int a,int b){
	    int c=a/b;
	    System.out.println("The divided value is "+c);
	}
}
